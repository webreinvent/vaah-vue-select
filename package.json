{
  "name": "vaah-vue-select",
  "version": "1.0.0",
  "description": "Simple Select Vue Component",
  "main": "dist/my-component.umd.js",
  "module": "dist/my-component.esm.js",
  "unpkg": "dist/my-component.min.js",
  "browser": {
    "./sfc": "src/VaahVueSelect.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webreinvent/vaah-vue-select.git"
  },
  "keywords": [
    "VueJs",
    "Vue",
    "Select",
    "Option",
    "Dropdown"
  ],
  "author": "WebReinvent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webreinvent/vaah-vue-select/issues"
  },
  "homepage": "https://github.com/webreinvent/vaah-vue-select#readme"
}
